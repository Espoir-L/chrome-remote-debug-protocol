{
  "name": "chrome-remote-debug-interface",
  "version": "1.0.0",
  "description": "Auto generate crdi.d.ts interface from protocol.json files used by Chrome Remote Debugging Protocol",
  "main": "out/example.js",
  "scripts": {
    "download-protocols": "jake -f jakefile.coffee download-protocols",
    "generate-crdi": "tsc -p generator/tsconfig.json && node out/generator/protocolToCrdi.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nojvek/chrome-remote-debug-interface"
  },
  "keywords": [
    "chrome",
    "debug",
    "protocol",
    "interface",
    "json",
    "generator",
    "typescript",
    "crdi",
    "adapter",
    "client",
    "converter"
  ],
  "author": "nojvek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nojvek/chrome-remote-debug-interface/issues"
  },
  "homepage": "https://github.com/nojvek/chrome-remote-debug-interface#readme",
  "dependencies": {
    "es6-simple-async": "git+https://github.com/Jamesernator/es6-simple-async.git",
    "node-fetch": "^1.5.3",
    "promisify-node": "^0.4.0",
    "ws": "^1.1.1"
  }
}
